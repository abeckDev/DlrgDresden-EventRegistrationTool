{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch and Debug Standalone Blazor WebAssembly App",
      "type": "blazorwasm",
      "request": "launch",
      "preLaunchTask": "build (BlazorApp)",
      "browser": "edge",
      "cwd": "${workspaceFolder}/AbeckDev.Dlrgdd.RegistrationTool.Frontend"
    },
    {
      "name": ".NET Core Launch (web)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (BlazorApp)",
      "program": "${workspaceFolder}/AbeckDev.Dlrgdd.RegistrationTool.Frontend/bin/Debug/net5.0/AbeckDev.Dlrgdd.RegistrationTool.Frontend.dll",
      "args": [],
      "cwd": "${workspaceFolder}/AbeckDev.Dlrgdd.RegistrationTool.Frontend",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/AbeckDev.Dlrgdd.RegistrationTool.Frontend/Views"
      }
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ]
}